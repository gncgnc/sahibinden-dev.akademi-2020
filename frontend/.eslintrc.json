{
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "no-return-assign": 0,
        "camelcase": 0,
        "no-unused-vars": 0,
        "linebreak-style": 0,
        "quotes": 0
    }
  }
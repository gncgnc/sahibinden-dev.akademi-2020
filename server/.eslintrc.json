{
    "env": {
        "jest": true,
    },
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "no-return-assign": 0,
        "camelcase": 0,
        "no-unused-vars": 0,
        "linebreak-style": 0,
        "quotes": 0,
        "no-console": 0
    }
}
